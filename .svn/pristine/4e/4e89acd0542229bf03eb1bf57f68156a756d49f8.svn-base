@CHARSET "UTF-8";

#content{
	width:900px;
	margin:10px auto 30px;
	border:1px solid #EBEBEB;
	padding:0 25px;
}

#content>h2{
	text-align:center;
	font-size:24px;
}
#content>h2+div{
	text-align:center;
	border-bottom:2px solid #ebebeb;
	padding-bottom:3px;
	font-size:13px;
	color: #999;
}

#content>article{
	padding:10px;
}