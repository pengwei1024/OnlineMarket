	</servlet>
	<servlet>
		<servlet-name>OrderListServlet</servlet-name>
		<servlet-class>com.cxft.onlinemarket.controller.OrderListServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>DelBookInfoInAdminServlet</servlet-name>
		<servlet-class>com.cxft.onlinemarket.controller.DelBookInfoInAdminServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>DisplayBookServlet</servlet-name>
		<servlet-class>com.cxft.onlinemarket.controller.DisplayBookServlet</servlet-class>
	</servlet>
  <servlet>
    <servlet-name>AccouCartServlet</servlet-name>
    <servlet-class>com.cxft.onlinemarket.controller.AccouCartServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ShowUserInfoServlet</servlet-name>
    <servlet-class>com.cxft.onlinemarket.controller.ShowUserInfoServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ChangeUserInfoServlet</servlet-name>
    <servlet-class>com.cxft.onlinemarket.controller.ChangeUserInfoServlet</servlet-class>
  </servlet>









	<servlet-mapping>
		<servlet-name>AddToCartServlet</servlet-name>
		<url-pattern>/AddToCartServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>DelCartServlet</servlet-name>
		<url-pattern>/DelCartServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>DelAllCartServlet</servlet-name>
		<url-pattern>/DelAllCartServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>DelCartInPageServlet</servlet-name>
		<url-pattern>/DelCartInPageServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ChangeNumServlet</servlet-name>
		<url-pattern>/ChangeNumServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>AddBookInfoServlet</servlet-name>
		<url-pattern>/AddBookInfoServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>QueryBookInfoServlet</servlet-name>
		<url-pattern>/QueryBookInfoServlet</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>OrderListServlet</servlet-name>
		<url-pattern>/OrderListServlet</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>DelBookInfoInAdminServlet</servlet-name>
		<url-pattern>/DelBookInfoInAdminServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>DisplayBookServlet</servlet-name>
		<url-pattern>/DisplayBookServlet</url-pattern>
	</servlet-mapping>
  <servlet-mapping>
    <servlet-name>AccouCartServlet</servlet-name>
    <url-pattern>/AccouCartServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ShowUserInfoServlet</servlet-name>
    <url-pattern>/ShowUserInfoServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ChangeUserInfoServlet</servlet-name>
    <url-pattern>/ChangeUserInfoServlet</url-pattern>
  </servlet-mapping>

	<listener>
		<listener-class>com.cxft.onlinemarket.service.MySessionListener</listener-class>
	</listener>
	<filter>
		<filter-name>CharsetFilter</filter-name>
		<filter-class>com.cxft.onlinemarket.filter.CharsetFilter</filter-class>
	</filter>
	<filter-mapping>
		<filter-name>CharsetFilter</filter-name>
		<url-pattern>*.jsp</url-pattern>
	</filter-mapping>
</web-app>