<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	metadata-complete="false"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
	<error-page>
		<error-code>404</error-code>
		<location>/error/404.jsp</location>
	</error-page>
	<error-page>
		<exception-type>java.lang.Throwable</exception-type>
		<location>/error/500.jsp</location>
	</error-page>
	<servlet>
		<servlet-name>LoginServlet</servlet-name>
		<servlet-class>com.cxft.onlinemarket.controller.LoginServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>RegisterServlet</servlet-name>
		<servlet-class>com.cxft.onlinemarket.controller.RegisterServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>AdminLoginCheck</servlet-name>
		<servlet-class>com.cxft.onlinemarket.controller.AdminLoginServlet</servlet-class>
	</servlet>
	<jsp-config>
		<taglib>
			<taglib-uri>/WEB-INF/custom.tld</taglib-uri>
			<taglib-location>/WEB-INF/custom.tld</taglib-location>
		</taglib>
	</jsp-config>
	<servlet>
		<servlet-name>LogoutServlet</servlet-name>
		<servlet-class>com.cxft.onlinemarket.controller.LogoutServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>LoginServlet</servlet-name>
		<url-pattern>/Login</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>RegisterServlet</servlet-name>
		<url-pattern>/Register</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>LogoutServlet</servlet-name>
		<url-pattern>/Logout</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>ValiImgServlet</servlet-name>
		<servlet-class>com.cxft.onlinemarket.controller.ValiImgServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>BookListServlet</servlet-name>
		<servlet-class>com.cxft.onlinemarket.controller.BookListServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ValiImgServlet</servlet-name>
		<url-pattern>/ValiImg</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>BookListServlet</servlet-name>
		<url-pattern>/BookListServlet</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>BookInfoServlet</servlet-name>
		<servlet-class>com.cxft.onlinemarket.controller.BookInfoServlet</servlet-class>
	</servlet>
	<servlet>
		<description>use to active user by email</description>
		<display-name>ActiveUser</display-name>
		<servlet-name>ActiveUser</servlet-name>
		<servlet-class>com.cxft.onlinemarket.controller.ActiveUser</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>BookInfoServlet</servlet-name>
		<url-pattern>/BookInfoServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ActiveUser</servlet-name>
		<url-pattern>/servlet/ActiveUser</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>BookListServlet</welcome-file>
	</welcome-file-list>
	<servlet>
		<servlet-name>OrderServlet</servlet-name>
		<servlet-class>com.cxft.onlinemarket.controller.OrderServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>OrderServlet</servlet-name>
		<url-pattern>/OrderServlet</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>AddToCartServlet</servlet-name>
		<servlet-class>com.cxft.onlinemarket.controller.AddToCartServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>DelCartServlet</servlet-name>
		<servlet-class>com.cxft.onlinemarket.controller.DelCartServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>DelAllCartServlet</servlet-name>
		<servlet-class>com.cxft.onlinemarket.controller.DelAllCartServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>DelCartInPageServlet</servlet-name>
		<servlet-class>com.cxft.onlinemarket.controller.DelCartInPageServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ChangeNumServlet</servlet-name>
		<servlet-class>com.cxft.onlinemarket.controller.ChangeNumServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>AddBookInfoServlet</servlet-name>
		<servlet-class>com.cxft.onlinemarket.controller.AddBookInfoServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>QueryBookInfoServlet</servlet-name>
